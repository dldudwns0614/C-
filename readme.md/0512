#include<stdio.h>    //구구단

int main()
{
    int dan = 2;
    int count = 1;

    while(count <= 9)
    {
        printf("%d * %d = %d\n", dan, count, dan*count);
        count = count + 1;
    }
    
}
////////////////////////////  
#include<stdio.h>       //1부터 100까지 더하기

int main()
{
    int sum = 0;
    int count = 0;
    
    do{
        sum = sum + count;
        count++;
    }while(count <= 100);
    
    printf("%d\n", sum);
    return 0;
}
