#include <stdio.h>  ////별 찍기 for문 이중중첩
int main()
{
    for(int i=1; i<=10; i++)
    {
        for(int j=1; j<=30; j++)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}

////////////////////////

#include <stdio.h>   ///for while중첩
int main()
{
    int j=0;
    for(int i=1; i<=10; i++)
    {
        while(j<30)
        {
            printf("*");
            j++;
        }
        j=0;
        printf("\n");
    }
    return 0;
}
